        n1=sc.nextInt();
        System.out.print("Enter 2nd number: ");
        n2=sc.nextInt();
        
        n1=n1+n2;
        n2=n1-n2;
        n1=n1-n2;
        System.out.println("After swapping:\nn1= "+n1+", n2= "+n2);
    }     
