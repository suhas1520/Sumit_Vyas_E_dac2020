        if(gen=='M' && age>=21)
            System.out.println("You are ELIGIBLE for marriage.");
        else if(gen=='F' && age>=18)
            System.out.println("You are ELIGIBLE for marriage.");
        else
            System.out.println("You are NOT ELIGIBLE for marriage.");
    }     
